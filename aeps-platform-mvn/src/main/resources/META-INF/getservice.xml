<xsd:element name="cellphones" type="tns:getInfoTicketArray"/>
</xsd:all>
</xsd:complexType>
<xsd:complexType name="InfoPhone">
<xsd:all>
<xsd:element name="status" type="xsd:string"/>
<xsd:element name="number" type="xsd:string"/>
