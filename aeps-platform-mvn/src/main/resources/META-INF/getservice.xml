<xsd:element name="desc" type="xsd:string"/>
<xsd:element name="dlr" type="xsd:string"/>
<xsd:element name="dlrDate" type="xsd:string"/>
</xsd:all>
</xsd:complexType>
</xsd:schema>
</types>
<message name="newSendSMSRequest">
<part name="user" type="xsd:string"/>
<part name="password" type="xsd:string"/>
<part name="teldestino" type="xsd:string"/>
<part name="mensaje" type="xsd:string"/>
</message>
<message name="newSendSMSResponse">
<part name="return" type="tns:WSResSendSMS"/>
</message>
<message name="programSMSRequest">
<part name="user" type="xsd:string"/>
<part name="password" type="xsd:string"/>
<part name="teldestino" type="xsd:string"/>
<part name="mensaje" type="xsd:string"/>
<part name="fecha" type="xsd:string"/>
</message>
