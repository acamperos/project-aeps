<output>
<soap:body use="literal" namespace="https://smsapp.sigmamovil.com:443/wsdl/getservice?wsdl/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
</output>
</operation>
</binding>
<service name="SigmaMovilService">
